{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "calendar_event": {
      "type": "object",
      "properties": {
        "external_id": {
          "type": "string"
        },
        "kind": {
          "type": "string"
        }
      },
      "required": [
        "external_id",
        "kind"
      ]
    },
    "created_at": {
      "type": "string"
    },
    "end_time": {
      "type": "string"
    },
    "event_guests": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "created_at": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "updated_at": {
            "type": "string"
          }
        },
        "required": [
          "created_at",
          "email",
          "updated_at"
        ]
      }
    },
    "event_memberships": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "user": {
            "type": "string"
          },
          "user_email": {
            "type": "string"
          },
          "user_name": {
            "type": "string"
          }
        },
        "required": [
          "user",
          "user_email",
          "user_name"
        ]
      }
    },
    "event_type": {
      "type": "string"
    },
    "invitees_counter": {
      "type": "object",
      "properties": {
        "active": {
          "type": "integer"
        },
        "limit": {
          "type": "integer"
        },
        "total": {
          "type": "integer"
        }
      },
      "required": [
        "active",
        "limit",
        "total"
      ]
    },
    "location": {
      "type": "object",
      "properties": {
        "join_url": {
          "type": "string"
        },
        "status": {
          "type": "string"
        },
        "type": {
          "type": "string"
        }
      },
      "required": [
        "join_url",
        "status",
        "type"
      ]
    },
    "meeting_notes_html": {
      "type": [
        "null",
        "string"
      ]
    },
    "meeting_notes_plain": {
      "type": [
        "null",
        "string"
      ]
    },
    "name": {
      "type": "string"
    },
    "start_time": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "updated_at": {
      "type": "string"
    },
    "uri": {
      "type": "string"
    }
  }
}
