{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "collection": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "calendar_event": {
            "type": "object",
            "properties": {
              "external_id": {
                "type": "string"
              },
              "kind": {
                "type": "string"
              }
            },
            "required": ["external_id", "kind"]
          },
          "created_at": {
            "type": "string",
            "format": "date-time"
          },
          "end_time": {
            "type": "string",
            "format": "date-time"
          },
          "event_guests": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "created_at": {
                  "type": "string",
                  "format": "date-time"
                },
                "email": {
                  "type": "string",
                  "format": "email"
                },
                "updated_at": {
                  "type": "string",
                  "format": "date-time"
                }
              },
              "required": ["created_at", "email", "updated_at"]
            }
          },
          "event_memberships": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "user": {
                  "type": "string",
                  "format": "uri"
                },
                "user_email": {
                  "type": "string",
                  "format": "email"
                },
                "user_name": {
                  "type": "string"
                }
              },
              "required": ["user", "user_email", "user_name"]
            }
          },
          "event_type": {
            "type": "string",
            "format": "uri"
          },
          "invitees_counter": {
            "type": "object",
            "properties": {
              "active": {
                "type": "integer"
              },
              "limit": {
                "type": "integer"
              },
              "total": {
                "type": "integer"
              }
            },
            "required": ["active", "limit", "total"]
          },
          "location": {
            "type": "object",
            "properties": {
              "join_url": {
                "type": "string",
                "format": "uri"
              },
              "status": {
                "type": "string"
              },
              "type": {
                "type": "string"
              }
            },
            "required": ["join_url", "status", "type"]
          },
          "meeting_notes_html": {
            "type": ["null", "string"]
          },
          "meeting_notes_plain": {
            "type": ["null", "string"]
          },
          "name": {
            "type": "string"
          },
          "start_time": {
            "type": "string",
            "format": "date-time"
          },
          "status": {
            "type": "string"
          },
          "updated_at": {
            "type": "string",
            "format": "date-time"
          },
          "uri": {
            "type": "string",
            "format": "uri"
          }
        }
      }
    }
  }
}
